             <h1>To Do list <%= listTitle %></h1>
        <% }else{ %>
             <h1>To Do list <%= listTitle %></h1>
        <% } %>

        <form class="itemForm" method="post" action="/">
            <input type="text" name="item" autocomplete="off">
            <button type="submit" name="listForm" value="<%= listTitle %>">+</button>
        </form>


